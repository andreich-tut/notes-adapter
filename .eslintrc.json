{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-tabs": 0,
        "no-console": 0,
        "new-cap": 0,
        "no-trailing-spaces": 0,
        "no-unused-vars": 0,
        "linebreak-style": 0,
        "eol-last": 0,
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "import/prefer-default-export": 0,
        "react/display-name": 0,
        "react-hooks/rules-of-hooks": "error",
        "react-hooks/exhaustive-deps": "warn"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react",
        "babel"
    ],
    "settings": {
        "react": {
            "version": "latest"
        }
    }
}